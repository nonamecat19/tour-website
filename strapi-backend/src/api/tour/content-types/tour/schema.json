{
  "kind": "collectionType",
  "collectionName": "tours",
  "info": {
    "singularName": "tour",
    "pluralName": "tours",
    "displayName": "Tour",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "text": {
      "type": "richtext"
    },
    "price": {
      "default": 0,
      "type": "integer"
    },
    "numberOfPeople": {
      "type": "integer",
      "default": 1
    },
    "cities": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::city.city",
      "inversedBy": "tours"
    },
    "preview": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category"
    }
  }
}
